{
  "processors": ["typescript", "typos", "json", "yaml"],
  "plugins": [
    "merge-duplicate-imports",
    "merge-if-statements",
    "apply-early-return",
    "apply-optional-chaining",
    "apply-flat-map",
    "apply-at",
    "convert-apply-to-spread",
    "convert-commonjs-to-esm",
    "convert-to-arrow-function",
    "convert-object-assign-to-merge-spread",
    "convert-assignment-to-arrow-function",
    "convert-assignment-to-comparison",
    "convert-object-entries-to-array-entries",
    "remove-nested-blocks",
    "simplify-assignment",
    "simplify-ternary",
    "declare-imports-first",
    "declare-before-reference",
    "for-of",
    "types",
    "promises",
    "nodejs",
    "typescript",
    "try-catch",
    "maybe",
    "logical-expressions",
    "logical-expressions",
    "react-hooks",
    "apply-shorthand-properties",
    "apply-nullish-coalescing",
    "jest",
    "printer",
    "remove-useless-escape",
    "remove-useless-template-expressions"
  ],
  "printer": "putout",
  "ignore": ["**/*.js", "**/*.cjs"],
  "rules": {
    "remove-useless-return": "off",
    "remove-unused-variables":"off",
    "merge-duplicate-imports": "off",
    "merge-if-statements": "off",
    "apply-early-return": "off",
    "apply-optional-chaining": "off",
    "remove-useless-template-expressions": "on",
    "apply-flat-map": "off",
    "apply-at": "off",
    "convert-apply-to-spread": "off",
    "convert-commonjs-to-esm/require": "off",
    "convert-commonjs-to-esm/exports": "off",
    "convert-commonjs-to-esm/commons": "off",
    "convert-index-of-to-includes": "off",
    "convert-to-arrow-function": "off",
    "convert-object-assign-to-merge-spread": "off",
    "convert-assignment-to-arrow-function": "off",
    "convert-assignment-to-comparison": "off",
    "convert-object-entries-to-array-entries": "off",
    "remove-nested-blocks": "off",
    "simplify-assignment": "off",
    "simplify-ternary/value": "off",
    "simplify-ternary/spread": "off",
    "declare-imports-first": "off",
    "declare-before-reference": "off",
    "for-of/map": "off",
    "for-of/for-in": "off",
    "for-of/for-each": "off",
    "for-of/reduce": "off",
    "for-of/remove-unused-variables": "off",
    "for-of/remove-useless": "off",
    "for-of/remove-useless-array-from": "off",
    "for-of/remove-useless-variables": [
      "off",
      {
        "maxProperties": 4
      }
    ],
    "for-of/for": "off",
    "types/declare": "off",
    "types/convert-typeof-to-istype": "off",
    "types/remove-useless-conversion": "off",
    "types/remove-double-negations": "off",
    "types/remove-useless-typeof": "off",
    "types/apply-is-array": [
      "off",
      {
        "inline": true
      }
    ],
    "new/remove-useless": "on",
        "new/add-missing": "on",
        "remove-useless-escape": "on",
    "promises/add-missing-await": "on",
    "promises/apply-await-import": "on",
    "promises/apply-top-level-await": "on",
    "promises/remove-useless-resolve": "on",
    "promises/remove-useless-async": "on",
    "promises/remove-useless-await": "on",
    "promises/convert-reject-to-throw": "on",
    "promises/convert-new-promise-to-async": "on",
    "nodejs/convert-buffer-to-buffer-alloc": "on",
    "nodejs/convert-fs-promises": "on",
    "nodejs/convert-promisify-to-fs-promises": "on",
    "nodejs/convert-dirname-to-url": "on",
    "nodejs/convert-url-to-dirname": "off",
    "nodejs/convert-top-level-return": "on",
    "nodejs/declare": "on",
    "nodejs/declare-after-require": "on",
    "nodejs/remove-process-exit": "on",
    "typescript/apply-as-type-assertion": "on",
    "typescript/apply-utility-types": "on",
    "typescript/convert-generic-to-shorthand": "on",
    "typescript/remove-duplicates-from-union": "on",
    "typescript/remove-duplicates-interface-keys": "on",
    "typescript/remove-duplicates-exports": "on",
    "typescript/remove-useless-types-from-constants": "off",
    "typescript/remove-unused-types": "off",
    "typescript/remove-useless-types": "off",
    "typescript/remove-useless-parens": "off",
    "typescript/remove-useless-mapped-types": "off",
    "try-catch/await": "on",
    "try-catch/args": "on",
    "try-catch/declare": "on",
    "try-catch/expand-arguments": "on",
    "try-catch/sync": "on",
    "try-catch/async": "on",
    "maybe/array": "on",
    "maybe/empty-array": "on",
    "maybe/fn": "on",
    "maybe/noop": "on",
    "maybe/declare": "on",
    "react-hooks/apply-short-fragment": "on",
    "react-hooks/declare": "on",
    "react-hooks/rename-method-under-score": "on",
    "react-hooks/convert-state-to-hooks": "on",
    "react-hooks/remove-bind": "on",
    "react-hooks/remove-this": "on",
    "react-hooks/remove-react": "on",
    "react-hooks/convert-class-to-function": "on",
    "react-hooks/convert-component-to-use-state": "on",
    "react-hooks/convert-import-component-to-use-state": "on",
    "apply-shorthand-properties": [
      "on",
      {
        "ignore": []
      }
    ],
    "apply-nullish-coalescing": "on",
    "jest": "off",
    "printer/add-args": "on",
    "printer/apply-breakline": "on",
    "printer/apply-linebreak": "on",
    "printer/apply-computed-print": "off",
    "printer/remove-args": "off"
  }
}
